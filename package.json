{
  "name": "adamite",
  "displayName": "Adamite",
  "description": "Annotations",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:adamite.launch"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "adamite.launch",
        "title": "Adamite: Launch Adamite"
      },
      {
        "command": "adamite.sel",
        "title": "Adamite: Create Annotation"
      },
      {
        "command": "adamite.addQuestion",
        "title": "Adamite: What is the intent of this code?"
      }
    ],
    "keybindings": [
      {
        "command": "adamite.sel",
        "key": "ctrl+alt+s",
        "mac": "cmd+alt+s"
      },
      {
        "command": "adamite.addQuestion",
        "key": "ctrl+alt+q",
        "mac": "cmd+alt+q"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm-run-all compile:*",
    "watch": "npm-run-all -p watch:*",
    "compile:extension": "tsc -p ./",
    "compile:views": "webpack --mode development",
    "watch:extension": "tsc -watch -p ./",
    "watch:views": "webpack --watch --mode development",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint source --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/uniqid": "^5.3.1",
    "@types/uuid": "^8.3.1",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "uniqid": "^5.3.0",
    "vscode-test": "^1.5.2",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@types/react-icons": "^3.0.0",
    "create-react-app": "^4.0.3",
    "css-loader": "^6.2.0",
    "css-modules-typescript-loader": "^4.0.1",
    "esnext": "^3.3.1",
    "firebase": "^8.10.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.4",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "shiki": "^0.9.5",
    "style-loader": "^3.2.1",
    "uuid": "^8.3.2"
  }
}
